<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebApp</title>
     <link rel = "stylesheet" href = "https://cdn.jsdelivr.net/npm/holiday.css@0.9.8"/>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/WebApp.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav>
       
                    <ul>
                        <li>
                            <a  asp-area="" asp-page="/Index">Home</a>
                        </li>

                        <li>
                           <span>Samples</span>
                            <ul>
                                <li><a asp-area="" asp-page="/Samples/CRUD">CRUD</a></li>
                                <li><a asp-area="" asp-page="/Samples/Query">Query</a></li>
                       

                            </ul>
                       </li>  
                        <li>
                            <a asp-area="" asp-page="/Privacy">Privacy</a>
                        </li>
                    </ul>
                
            
        </nav>
    </header>
    <div>
        <main role="main">
            @RenderBody()
        </main>
    </div>

    <footer>
      
            &copy; 2022 - WebApp - <a asp-area="" asp-page="/Privacy">Privacy</a>
     
    </footer>

   
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>